public class Login {
	@Test(priority=1)
	public static void main(String[] args) {
		
			// TODO Auto-generated method stub
	         WebDriver driver=new ChromeDriver();
	         driver.manage().window().maximize();
	        driver.get(" https://demo.dealsdray.com/");
